{
  "name": "netlify-fauna",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "faunadb": "^2.5.2",
    "http-proxy-middleware": "^0.19.0",
    "immer": "^1.7.4",
    "netlify-identity-widget": "^1.4.14",
    "react": "^16.7.0-alpha.2",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "^16.7.0-alpha.2",
    "react-scripts": "^2.1.1"
  },
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "docs": "md-magic --path '**/*.md' --ignore 'node_modules'",
    "checkForFaunaKey": "node ./scripts/check-for-fauna-key.js",
    "start": "npm-run-all --parallel checkForFaunaKey start:app start:server",
    "start:app": "react-scripts start",
    "start:server": "netlify-lambda serve functions -c ./webpack.config.js",
    "prebuild": "echo 'setup faunaDB' && npm run bootstrap",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "oldbuild:functions": "netlify-lambda build functions -c ./webpack.config.js",
    "build:functions": "npm run zip",
    "zip": "cd functions/identity-signup && npm install && zip -r identity-signup.zip *",
    "postzip": "mkdir -p functions-build && mv functions/identity-signup/identity-signup.zip functions-build"
  },
  "devDependencies": {
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "babel-loader": "^8.0.4",
    "markdown-magic": "^0.1.23",
    "netlify-lambda": "^1.0.3",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
